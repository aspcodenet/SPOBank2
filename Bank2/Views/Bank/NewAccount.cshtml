@model Bank2.ViewModels.EditAccountViewModel
@{
    ViewBag.Title = "NewAccount";
}

@using (Html.BeginForm("SaveNewAccount", "Bank", FormMethod.Post))
{
    @Html.LabelFor(m => m.Fornamn)
    @Html.TextBoxFor(m => m.Fornamn)

    @Html.LabelFor(m => m.Efternamn)
    @Html.TextBoxFor(m => m.Efternamn)

    @Html.LabelFor(m => m.City)
    @Html.TextBoxFor(m => m.City)

    @Html.LabelFor(m => m.Birthyear)
    @Html.TextBoxFor(m => m.Birthyear)

    <input type="submit" value="Spara" />
}




